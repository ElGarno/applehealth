      "targets": [
        {
          "datasource": { "type": "influxdb", "uid": "af6pid1r7xp1cf" },
          "query": "// Aktueller HRV-Wert\ncurrentHRV = from(bucket: \"apple_health\")\n  |> range(start: -3d)\n  |> filter(fn: (r) => r._measurement == \"health_metrics\")\n  |> filter(fn: (r) => r.metric == \"heart_rate_variability\")\n  |> filter(fn: (r) => r._field == \"value\")\n  |> last()\n  |> findRecord(fn: (key) => true, idx: 0)\n\n// 7-Tage-Durchschnitt\nbaselineHRV = from(bucket: \"apple_health\")\n  |> range(start: -14d, stop: -1d)\n  |> filter(fn: (r) => r._measurement == \"health_metrics\")\n  |> filter(fn: (r) => r.metric == \"heart_rate_variability\")\n  |> filter(fn: (r) => r._field == \"value\")\n  |> mean()\n  |> findRecord(fn: (key) => true, idx: 0)\n\n// Recovery Score berechnen (HRV relativ zur Baseline)\nrecoveryRatio = if baselineHRV._value > 0.0 then currentHRV._value / baselineHRV._value else 1.0\n\n// Auf 0-100 Skala normalisieren (80% = 40 Punkte, 100% = 70 Punkte, 120%+ = 100 Punkte)\nscore = if recoveryRatio < 0.8 then recoveryRatio * 50.0\n  else if recoveryRatio < 1.0 then 40.0 + (recoveryRatio - 0.8) * 150.0\n  else if recoveryRatio < 1.2 then 70.0 + (recoveryRatio - 1.0) * 150.0\n  else 100.0\n\nfrom(bucket: \"apple_health\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"health_metrics\")\n  |> filter(fn: (r) => r.metric == \"heart_rate_variability\")\n  |> limit(n: 1)\n  |> map(fn: (r) => ({r with _value: score}))",
          "refId": "A"
        }
      ],
      "title": "Trainingsbereitschaft",
      "type": "gauge"
    },
